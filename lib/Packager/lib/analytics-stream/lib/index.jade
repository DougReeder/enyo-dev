include mixins

doctype html
html(lang='en')
	head
		meta(charset='utf-8')
		meta(http-equiv='x-ua-compatible', content='ie=edge')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		title= package
		each stylesheet in stylesheets
			style!= stylesheet.contents
	body
		// this output is automatically generated by the enyo-dev/analytics output stream
		#header
			.title #{package} #[span.meta= build]
			.menu
				ul
					li: a(href='#overview') overview
					li: a(href='#bundles') bundles
					li: a(href='#modules') modules
					li: a(href='#styles') styles
					li: a(href='#assets') assets
		#overview
			a(name='overview')
			h1 Overview
			.table.stats
				table
					thead
						tr
							td
							td
							td ~size
					tbody
						tr
							td bundles
							td= bundles.length
							td 0
						tr
							td modules
							td= modules.length
							td 0
						tr
							td styles
							td 0
							td 0
						tr
							td assets
							td 0
							td 0
						tr
							td(colspan=2) total size
							td 0
			.graph.stats
		#bundles
			a(name='bundles')
			h1 Bundles
			.menu
				ul
					for bundle in bundles
						li: +bundleLink(bundle.name)
			.content
				for bundle in bundles
					+bundleSection(bundle)
		#modules
			a(name='moduled')
			h1 Modules
			.menu
				ul
					for bundle in bundles
						li
							h3: +bundleLink(bundle.name)
							ul
								for module in bundle.modules
									li: +moduleLink(module.name)
			.content
				for module in modules
					+moduleSection(module)


		each script in scripts
			if script.src
				script(src=script.src)
			else
				script!= script.contents